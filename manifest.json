{
  "manifest_version": 3,
  "name": "Tabby",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "action": {
    "default_title": "Tabby",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tab",
    "tabs",
    "tabGroups",
    "storage"
  ]
}